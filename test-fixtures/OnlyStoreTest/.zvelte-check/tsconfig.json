{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "noEmit": true,
    "skipLibCheck": true,
    "allowJs": true,
    "checkJs": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "rootDirs": [".", ".."]
  },
  "include": [
    "stubs.d.ts",
    "../src/**/*.ts",
    "../src/**/*.js",
    "StoreTest.svelte.ts"
  ],
  "exclude": ["../node_modules"]}
